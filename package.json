{
  "devDependencies": {
    "@graphql-codegen/typescript": "2.8.2",
    "@graphql-codegen/typescript-document-nodes": "2.3.7",
    "@graphql-codegen/typescript-resolvers": "2.7.7",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.43.0",
    "casual": "^1.6.2",
    "concurrently": "^7.5.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "graphql-config": "^4.3.6",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "@swc/jest": "^0.2.23",
    "jest": "^29.3.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@graphql-codegen/cli": "2.13.12",
    "@swc/core": "^1.3.18",
    "typescript": "*",
    "ts-node": "^10.9.1",
    "husky": "^8.0.0",
    "@apollo/server": "^4.1.1",
    "@graphql-tools/load-files": "^6.6.1",
    "@prisma/client": "^4.6.1",
    "ajv": "^8.11.2",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "argon2": "^0.30.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-schema": "^5.1.1",
    "express": "^4.18.2",
    "fast-jwt": "^2.0.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "pino": "^8.7.0",
    "prisma": "^4.6.1",
    "tsconfig-paths": "^4.1.0"
  },
  "scripts": {
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "debug": "npx nodemon --exec node --inspect -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "dev": "concurrently \"npm run debug\" \"npm run graphql:codegen -- --watch\"",
    "prestart": "tsc -p tsconfig.production.json",
    "start": "ts-node -r tsconfig-paths/register build/src/index.js",
    "bootstrap": "npm run db:start && npm run db:generate && npm run graphql:codegen",
    "db:start": "docker-compose up -d database",
    "db:generate": "prisma migrate reset && prisma generate",
    "db:migrate": "prisma migrate dev && prisma generate",
    "prepare": "husky install",
    "test": "jest"
  }
}
